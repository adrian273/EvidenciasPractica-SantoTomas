<table border="0" height="100%" width="100%" cellpadding="0" cellspacing="4"> 
 <form id="gform" method="POST">
  <tr>
    <td colspan="2" height="10"></td>
  </tr> 
  <tr>
    <td class="ftitle">Electronic Password</td>
  </tr>
	<td nowrap style="font-size: 10px" height="30">
	{if $error_string}
		<span style="color: red">Incorrect Electronic Password</span>
	{/if}
	</td>
 
  <tr>
    <td>
    	<input type="password" id="user_epass" name="user_epass" value="{$user_epass}"> *
    </td>
  </tr>
  <tr> 
    <td valign="bottom" height="100%">
  		<input type="submit" name="submit" class="btn" value="{$grl_send}">
  		<input type="button" class="btn" value="{$grl_cancel}" onclick="parent.dhxWins.window('w1').close()">
    </td>
  </tr>
 </form>
</table> 

	<script language="JavaScript" type="text/javascript">
  		document.getElementById('user_epass').focus();
  		
  	</script>

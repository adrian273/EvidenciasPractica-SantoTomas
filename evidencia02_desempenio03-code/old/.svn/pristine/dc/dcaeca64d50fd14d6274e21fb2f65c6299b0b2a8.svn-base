	function add_date( $fecha_original , $anios , $meses , $dias )
	{
		$dia			= substr($fecha_original,8,2);
		$mes			= substr($fecha_original,5,2);
		$anio			= substr($fecha_original,0,4);
		//
		return ( date ("Y-m-d", mktime (0,0,0,$mes + $meses ,$dia + $dias ,$anio + $anios )) );
	}



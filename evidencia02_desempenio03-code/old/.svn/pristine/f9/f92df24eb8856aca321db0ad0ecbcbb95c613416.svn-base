<table border="0" width="100%" height="100%" cellpadding="0" cellspacing="4"> 
 <form id="gform" method="POST" action="{$index_url}{$resource_path}/edit/{$patient_id}">
  <tr>		
		<td height="10">Patient: </td>
    <td height="10">
    	{$patient_name}
    </td>
  </tr>
  <tr>		
		<td height="10">Billing Factor: *</td>
    <td height="10">
    	<input type="text" name="billing_factor" value="{$billing_factor}" id="billing_factor"  size="6" maxlength="6"/>
    </td>
  </tr>
  <tr>
    <td colspan="2" height="100%" valign="bottom">
    	<input type="submit" class="btn" value="Save">
    	<input type="button" class="btn" value="{$grl_cancel}" onclick="parent.dhxWins.window('w2').close()">
    </td>
  </tr>
</table> 

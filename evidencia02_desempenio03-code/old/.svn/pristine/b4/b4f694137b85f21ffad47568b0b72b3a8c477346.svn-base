    function alter_discharge(){
      if(!document.getElementById('p2_discha').checked){
        var optionts = document.tform.p2_tab_064_discha_reason;
        for(i in optionts){
          optionts[i].disabled = true;
        }
        document.getElementById('p2_discha_other').disabled = true;
      }else{
          
        var optionts = document.tform.p2_tab_064_discha_reason;
        for(i in optionts){
          optionts[i].disabled = false;
        }
        document.getElementById('p2_discha_other').disabled = false;            
      }
    }

    function is_assessment_other(){
      var asses_val = 0;
      asses_val = document.getElementById('tab_92_assess_type').value;
      
      if(asses_val == 5){   
        $('#planofcare-fields input').each(function(){
          if ($(this).attr('name')=='p2_discharge_summary') {
            $(this).attr('checked', true);
          } else {
            switch($(this).getType()){
              case 'radio':                         
              case 'checkbox':      
                $(this).removeAttr('checked');
                break;
              case 'select':                                
                $(this).removeAttr('selected');
                if($(this).children('option').length > 0){                          
                  $(this).children('option:first').attr('selected','selected');
                }                           
                break;
              default:
                $(this).val('');
                break;                                
            }
          }

          $('#p2_discha').attr('checked', true);
          alter_discharge();
          $('#p2_dis_instructions').removeAttr('disabled');
          
        });

      } else {
        $('#p2_discha').removeAttr('checked'); 
        alter_discharge();
        $('#p2_dis_instructions').attr('disabled', 'disabled');
      }
      
    }

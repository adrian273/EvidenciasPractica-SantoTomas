	});

	$(".dc_plan").change(function() {
		validate_checkbox('dc_planning', 'dc_plan');
	});

	$('#rom_na').change(function() {
		romStrength('rom_na', 'rom_na_style');
	});

	$('#strength_na').change(function() {
		romStrength('strength_na', 'strength_na_style');
	})

	function validate_modalities_na() {
		if ($("#modalities_na").is(':checked')) {
			$("#modalities_text").attr('disabled', true);
		} else {
			$("#modalities_text").attr('disabled', false);
		}
 	}

	 

	 function validate_checkbox(name_id,name_class) {
		$("#"+name_id).val(0);
		$("."+name_class).each(function() {
			if(this.checked) {
				$("#"+name_id).val(1);
			} else {
					$("#"+name_id).val();
			}
		});
	}

	function checkDataPT(dom) {
		if($("input[name="+dom+"]").is(':checked')) {
			$("input[name="+dom+"]").attr('checked', false)
		} else {
			$("input[name="+dom+"]").attr('checked', true)
		}
	}

	function romStrength(id, class_name) {
		if ($("#"+id).is(':checked')) {
			$("."+class_name).css('visibility', 'hidden');
		} else {
			$("."+class_name).css('visibility', 'visible');
		}
	}


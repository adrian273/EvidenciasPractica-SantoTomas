 include('visitform.php');
 
 class snpn extends VisitForm {
	
	function snpn () {
	
		parent::VisitForm();
		$this->load->model('snpnmdl');
		$this->load->model('snwamdl');
		$this->tpl->assign("resource_path", "task/snpn");
		$this->tpl->assign(array("tab_page" => true));
		$this->tpl->assign('tab_25_list', $this->parametermdl->getByType(25));
		$this->tpl->assign('tab_26_list', $this->parametermdl->getByType(26));
		$this->tpl->assign('tab_30_list', $this->parametermdl->getByType(30));
		
	}

	function index ( $visit_log_id, $action='task' ) {

		$this->tpl->assign('task_page', 4);
		$visitlog = $this->visitlogmdl->getById($visit_log_id);
		$this->assignObject($visitlog);
		$this->assignObject($this->snpnmdl->get($visit_log_id));
		$v11nursevisitid = $this->snpnmdl->get($visit_log_id);
		if (count($v11nursevisitid)) $this->assignObject($this->snwamdl->get($v11nursevisitid->v11_nurse_visit_id));

		if ($this->validation->run() == TRUE) {
			
			if($this->updateFormEdition($visit_log_id)){
				$this->assignPostData($this->snpnmdl);
				$this->assignPostData($this->snwamdl);
				
				$this->snpnmdl->GastroLastBM = standard_date(human_to_unix($this->snpnmdl->GastroLastBM), 'MYSQL');

				$v11_nurse_visit_id = 0;
				if (count($snpn = $this->snpnmdl->get($visit_log_id))) {
					$this->snpnmdl->update($visit_log_id);
					$v11_nurse_visit_id = $snpn->v11_nurse_visit_id;
				} else {
					$this->snpnmdl->insert($visit_log_id);
					$v11_nurse_visit_id = $this->snpnmdl->v11_nurse_visit_id;
				}

				if (count($snwa = $this->snwamdl->get($v11_nurse_visit_id))) {
					$this->snwamdl->update($v11_nurse_visit_id);
				} else {
					$this->snwamdl->insert($v11_nurse_visit_id);	
				}

						
				$snpn = $this->snpnmdl->get($visit_log_id);
				$snwa = $this->snwamdl->get($snpn->v11_nurse_visit_id);

				$this->load->helper('ajax');
				if (isAjax()) {
					echo "1";
					exit;
				}

				$this->tpl->assign('GastroLastBM', standard_date(mysql_to_unix($snpn->GastroLastBM), 'USA_DATE'));
			}

			$this->formRedirection($visit_log_id);
			
		}
		
		$this->tpl->assign('emp_name_list', $this->teammdl->getAll($visitlog->patient_id, array(1, 8, 12)));
		$this->getPatientInfo($visit_log_id);
		$this->tpl->assign("visit_log_id", $visit_log_id);
		$this->tpl->assign_include("dynamic_form", "task/documents/snpn");
		$this->setfbase($visit_log_id, $action);
		$this->tpl->view("parts/ibase", $this->lang->language);
		
	}



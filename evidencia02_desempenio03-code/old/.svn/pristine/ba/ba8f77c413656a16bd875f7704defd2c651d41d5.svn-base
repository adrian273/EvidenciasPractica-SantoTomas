        w1.denyMove(); 
        dhxWins.window('w1').center();
        dhxWins.window('w1').setModal(true);
        w1.attachURL("{$index_url}task/aavi/pre_remove_document/" + visit_log_id);

        pos = dhxWins.window('w1').getPosition();
        dhxWins.window('w1').setPosition(pos[0], 60);

    {literal}}{/literal}    

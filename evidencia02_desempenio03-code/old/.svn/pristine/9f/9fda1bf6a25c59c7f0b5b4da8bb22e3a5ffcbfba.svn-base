	function validate_home_YN() {
		$(".home_YN").each(function() {
				if (this.checked) {
					if (this.value == '2') {
						$('.home_bound').each(function() {
							$(this).attr({'disabled': true, 'checked': false});
						});
					} else {
							$('.home_bound').each(function() {
							$(this).attr({'disabled': false});
						});
					}
				}
		}) 
	}

	function validate_checkbox(name_id,name_class) {
		$("#"+name_id).val(0);
		$("."+name_class).each(function() {
			if(this.checked) {
				$("#"+name_id).val(1);
			} else {
					$("#"+name_id).val();
			}
		});
	}

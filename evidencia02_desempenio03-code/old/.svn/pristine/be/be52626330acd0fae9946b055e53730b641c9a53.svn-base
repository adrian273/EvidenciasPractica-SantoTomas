$(document).ready(function(){
  // $('.ptev-goals').parent().parent().find(':input').attr('disabled', true);

  $('.ptev-goals').each(function(i, el){
      if($(el).attr('checked') == true ) {
          $(el).parent().parent().find(':input:not(.ptev-goals)').attr('disabled', false);

          $(el).parent().parent().find(':input:not(.ptev-goals)').each(function(j, el2){
              $('input:hidden[name='+$(el2).attr('name')+']').remove();
          });
      } else {
          $(el).parent().parent().find(':input:not(.ptev-goals)').each(function(j, el2){
              if ($(el2).attr('type') == 'checkbox') {
                  if($(el2).attr('checked') == true ) {
                    $('.ptev-goals-hidden').append('<input type="hidden" name="'+$(el2).attr('name')+'" value="'+$(el2).val()+'" >');
                  } else {
                    $('.ptev-goals-hidden').append('<input type="hidden" name="'+$(el2).attr('name')+'" value="" >');
                  }
              } else if($(el2).attr('type') == 'radio'){
                  if ($('input:hidden[name='+$(el2).attr('name')+']').length == 0) {
                      // when hidden field is not exist
                      if($('input[type=radio][name='+$(el2).attr('name')+']:checked').length > 0) {
                          // get actual radio option selected value and assign to a new created hidden field
                          $('.ptev-goals-hidden').append('<input type="hidden" name="'+$(el2).attr('name')+'" value="'+$('input[type=radio][name='+$(el2).attr('name')+']:checked').val()+'" >');
                      } else {
                          // actual radio selected value not found so assign empty value to a new created hidden field
                          $('.ptev-goals-hidden').append('<input type="hidden" name="'+$(el2).attr('name')+'" value="" >');
                      }
                  } else {
                      // when hidden field is already exist
                      if($('input[type=radio][name='+$(el2).attr('name')+']:checked').length > 0) {
                          // get actual radio option selected value and assign to hidden field
                          $('input:hidden[name='+$(el2).attr('name')+']').val($('input[type=radio][name='+$(el2).attr('name')+']:checked').val());
                      } else {
                          // actual radio selected value not found so assign empty value to hidden field
                          $('.ptev-goals-hidden').append('<input type="hidden" name="'+$(el2).attr('name')+'" value="" >');
                      }
                  }
              } else {
                  if ($('input:hidden[name='+$(el2).attr('name')+']').length == 0) {
                      $('.ptev-goals-hidden').append('<input type="hidden" name="'+$(el2).attr('name')+'" value="'+$(el2).val()+'" >');
                  } else {
                      $('input:hidden[name='+$(el2).attr('name')+']').val($(el2).val());
                  }

              }
          });

          $(el).parent().parent().find(':input:not(.ptev-goals)').attr('disabled', true);
      }
  });


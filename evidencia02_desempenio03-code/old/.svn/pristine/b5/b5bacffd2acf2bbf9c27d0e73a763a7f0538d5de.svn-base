<script>
{literal}
function Funtherapy() {
    //switchDisplay('therapy');
    setValue('ther_eval_plan', '');
    setValue('ther_notes', '');
	///selectOption('ther_supervisor', '');
    selectOption('ther_name', '');
    setCheck('ther_implem_plan', false);
    setCheck('ther_follow_plan', false);
    setCheck('ther_raport_pat_fam', false);
}
function ther_na_check(obj, type){
	switch(type){
		case 'selectbox': 
			if(obj.options[obj.selectedIndex].value != "")
				document.getElementById('ther_na').checked = false;
			break;
		case 'radio':	
			var radioLength = obj.length;
			if(radioLength == undefined)
				if(obj.checked){
					document.getElementById('ther_na').checked = false;
					break;
				}	
				else{
					break;
				}
					
				for(var i = 0; i < radioLength; i++) {
					if(obj[i].checked) {
						document.getElementById('ther_na').checked = false;
						break;
					}
				}
				break;
		default:
			if(obj.value != "")
				document.getElementById('ther_na').checked = false;
			break;
	}
}

if (document.tform.ther_na && document.tform.ther_na.checked==true)
{
   Funtherapy();
}

function cleanBelow(){
	if(document.tform.ther_na.checked){
		document.tform.ther_therapist_evaluated.value='';
		document.tform.ther_follow_plan_care_YN[0].checked=false;
		document.tform.ther_follow_plan_care_YN[1].checked=false;
		

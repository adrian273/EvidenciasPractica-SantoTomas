	function get ( $template_id ) {
		
		$this->db->where('tpl_ptev_id', $template_id);
	
		$query = $this->db->get('tpl_ptev');
		return $query->row();
		
	}
	
	function getByUserId ( $user_id ) {
		
		$this->db->where('user_id', $user_id);
	
		$query = $this->db->get('tpl_ptev');
		return $query->result2('tpl_ptev_id', 'tpl_ptev_name');
		
	}

	function insert ( $user_id, $template_name ) {

		$this->user_id = $user_id;
		$this->tpl_ptev_name = $template_name;
		
		$this->db->insert('tpl_ptev', $this);

		return $this->db->insert_id();
		
	}

	function update ( $tpl_ptev_id ) {
		unset($this->tpl_ptev_id);
		unset($this->user_id);
		$this->db->where('tpl_ptev_id', $tpl_ptev_id);
		$this->db->update('tpl_ptev', $this);
	}

	function updateFlex($tpl_ptev_id, $attribute, $value, $where = 'tpl_ptev_id') {
		$this->db->where($where, $tpl_ptev_id);
		$this->db->set($attribute, $value);

		$this->db->update('tpl_ptev');
	}	

	function delete ( $tpl_ptev_id, $user_id ) {
		$this->db->where('user_id', $user_id);
		$this->db->where('tpl_ptev_id', $tpl_ptev_id);
		$this->db->delete('tpl_ptev');
	}



<table border="0" height="100%" width="100%" cellpadding="0" cellspacing="4"> 
 <form id="gform" method="POST" action="{$index_url}{$resource_path}/exporttopc/{$RFA_export_ids}">
  <tr>
    <td class="ftitle" nowrap>Export Date:</td>
    <td> {$export_date}
		{* <input type="text" name="export_date" value="{$export_date}" size="28"/> *}
    </td>
  </tr>
  
  <tr>
    <td class="ftitle" nowrap>Export Notes :</td>
 
    <td height="100%" colspan="2">
		<input type="text" name="export_notes" value="{$export_notes}" size="28"/>
    </td>
  </tr>
  <tr> 
    <td valign="bottom" colspan="2">
  		<input type="submit" name="submit" class="btn" value="Confirm Export to PC">
  		<input type="button" class="btn" value="{$grl_cancel}" onclick="parent.dhxWins.unload()">
  		<input type="hidden" name="RFA_export_ids" value="{$RFA_export_ids}">
  		
    </td>
  </tr>
 </form>
</table> 
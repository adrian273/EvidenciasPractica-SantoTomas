/*****************************************************************************************************************************************************************
	BUTTON
*****************************************************************************************************************************************************************/
div table.dhtmlxToolbarTable_ModernRed td.itemButton {
	cursor: pointer;
	font-weight: bold;
	font-family: Arial;
	font-size: 12px;
	-moz-user-select: none;
}
div table.dhtmlxToolbarTable_ModernRed td.itemButton div.itemDefault {
	position: absolute;
	display: none;
}
div table.dhtmlxToolbarTable_ModernRed td.itemButton div.itemOver {
	position: absolute;
	border-top: #E0B0B0 1px solid;
	border-left: #CF8F8F 1px solid;
	border-right: #713C3C 1px solid;
	border-bottom: #713C3C 1px solid;
	background-image: url("../imgs/dhtmlxtoolbar/modernred/dhtmlxtoolbar_top_bg_over.gif");
	background-repeat: repeat-x;
	top: 2px;
	left: 0px;
	height: 23px;
	right: 0px;
	width= 100%;
}
div table.dhtmlxToolbarTable_ModernRed td.itemButton div.itemPressed {
	position: absolute;
	border-top: #8B3232 1px solid;
	border-left: #8B3232 1px solid;
	border-right: #CF8F8F 1px solid;
	border-bottom: #CF8F8F 1px solid;
	background-image: url("../imgs/dhtmlxtoolbar/modernred/dhtmlxtoolbar_top_bg_pressed.gif");
	background-repeat: repeat-x;
	top: 1px;
	left: 0px;
	height: 24px;
	right: 0px;
	width= 100%;
}
div table.dhtmlxToolbarTable_ModernRed td.itemButton div.dhtmlxToolbarButton {
	position: relative;
	height: 100%;
	color: #FFFFFF;
}
div table.dhtmlxToolbarTable_ModernRed td.itemButton div.dhtmlxToolbarButtonDisabled {
	position: relative;
	height: 100%;
	color: #CCCCCC;
}
div table.dhtmlxToolbarTable_ModernRed td.itemButton div.dhtmlxToolbarImage {
	position: relative;
	width: 16px;
	height: 16px;
	margin-top: 4px;
	margin-left: 2px;
	margin-right: 2px;
}
div table.dhtmlxToolbarTable_ModernRed td.itemButton div.dhtmlxToolbarText {
	position: relative;
	font-weight: bold;
	font-family: Arial;
	font-size: 12px;

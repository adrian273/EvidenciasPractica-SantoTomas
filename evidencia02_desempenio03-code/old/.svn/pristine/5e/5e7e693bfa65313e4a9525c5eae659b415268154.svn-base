/***************************************************************************************************************************************************************
								dhtmlxToolbar "MODERN RED" skin
***************************************************************************************************************************************************************/
table.dhtmlxToolbarTableOuter_ModernRed {
	height: 30px;
}
td.dhtmlxToolbarTopLeft_ModernRed {
	width: 8px;
	background-image: url("../imgs/dhtmlxtoolbar/modernred/dhtmlxtoolbar_top_bg_left.gif");
	background-repeat: no-repeat;
}
td.dhtmlxToolbarTopRight_ModernRed {
	width: 8px;
	background-image: url("../imgs/dhtmlxtoolbar/modernred/dhtmlxtoolbar_top_bg_right.gif");
	background-repeat: no-repeat;
}
div.dhtmlxToolbar_ModernRed {
	background-image: url("../imgs/dhtmlxtoolbar/modernred/dhtmlxtoolbar_top_bg_middle.gif");
	background-repeat: repeat-x;
	width: 1100px;
	height: 30px;
}
div.dhtmlxToolbarVer_ModernRed {
	position: absolute;
	top: 70px;
	left: 20px;
	/*
	background-image: url("../imgs/dhtmlxtoolbar/webbar_bg_ver.gif");
	background-repeat: repeat;
	*/
	background-color: #C5CBCC;
	/* width: 1%; */
	height: 600px;
	border: silver 1px solid;
	/* -moz-user-select: none; */
	overflow: hidden;
}
div.dhtmlxToolbar_ModernRed table.dhtmlxToolbarTable_ModernRed {
	width: 100%;
	height: 100%;
	/*
	border: #FFFFFF 1px solid;
	border-bottom: #E0E0E0 1px solid;
	*/
}
div.dhtmlxToolbarVer_ModernRed table.dhtmlxToolbarTable_ModernRed {
	@height: 100%;
	border: #FFFFFF 1px solid;
	border-bottom: #E0E0E0 1px solid;
	/* border-bottom: none; */
}
div table.dhtmlxToolbarTable_ModernRed td {
	white-space: nowrap;
	position: relative;
}
/*****************************************************************************************************************************************************************
	SEPARATOR
*****************************************************************************************************************************************************************/
div table.dhtmlxToolbarTable_ModernRed td.itemSeparator div.dhtmlxToolbarSeparator {
	position: relative;
	width: 0px;
	margin-left: 3px;
	margin-right: 3px;
	border-left: #8B3232 1px solid;
	border-right: #CF8F8F 1px solid;
	margin-top: -2px;
	#margin-top: 1px;
	font-size: 1px;
	cursor: default;
	height: 26px;
	-moz-user-select: none;
}
div table.dhtmlxToolbarTable_ModernRed td.itemSeparatorVert div.dhtmlxToolbarSeparator {
	padding-left: 3px;
	margin-right: 3px;
	border-bottom: #808080 1px solid;
	margin-top: 1px;
	font-size: 1px;
	cursor: default;
	height: 1px;
	-moz-user-select: none;
}
/*****************************************************************************************************************************************************************
	TEXT
*****************************************************************************************************************************************************************/
div table.dhtmlxToolbarTable_ModernRed td.itemText div.dhtmlxToolbarText {
	padding-left: 2px;
	padding-right: 2px;
	padding-top: 4px;
	font-size: 12px;
	font-weight: bold;
	font-family: Arial;
	color: #FFFFFF;
	cursor: default;

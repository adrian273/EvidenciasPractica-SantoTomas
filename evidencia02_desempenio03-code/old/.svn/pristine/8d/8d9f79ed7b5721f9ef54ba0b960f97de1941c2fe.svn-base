				}
			}
		}

		if ($gov_program == 'Y-Waiver'){
			if (($this->getAgencyType() == 'A' AND $visitlog->visit_status_id == 3) OR ($this->getAgencyType() == 'C' AND $visitlog->visit_status_id == 8)) {

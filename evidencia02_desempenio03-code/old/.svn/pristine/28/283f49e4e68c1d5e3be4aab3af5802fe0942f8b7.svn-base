<table border="0" width="100%" cellpadding="2" cellspacing="0"> 
  <input type="hidden" name="user_id" value="{$user_id}">
  <input type="hidden" name="password" value="{$password}">
  <tr>
    <td color= "green">{$msg}</td>
  </tr>
      
  <tr>
    <td colspan="4" height="10"></td>
  </tr> 
  <tr> 
    <td class="ftitle">User Type</td>
    <td><input type="text" name="tab_005_user_type" value="{$tab_005_user_type}"></td>
  </tr>
  <tr> 
    <td class="ftitle">Last Name</td>
    <td><input type="text" name="last_name" value="{$last_name}"></td>
    <td class="ftitle">First Name</td>
    <td><input type="text" name="first_name" value="{$first_name}"></td>
  </tr>
  <tr> 
    <td class="ftitle">Middle Initial</td>
    <td><input type="text" name="middle_initial" value="{$middle_initial}"></td>
  </tr>
  <tr>
    <td class="ftitle">Address</td>
    <td><input type="text" name="address" value="{$address}"></td>
    <td class="ftitle">City</td>
    <td><input type="text" name="city" value="{$city}"></td>
  </tr>
  <tr> 
    <td class="ftitle">State ID</td>
    <td><input type="text" name="state_id" value="{$state_id}"></td>
    <td class="ftitle">Zip Code</td>
    <td><input type="text" name="zip_code" value="{$zip_code}"></td>
  </tr>
  <tr>
    <td colspan="4" height="10"></td>
  </tr> 
  <tr> 
    <td class="ftitle">Primary Phone</td>
    <td><input type="text" name="phone_cell" value="{$phone_cell}"></td>
    <td class="ftitle">Email</td>
    <td><input type="text" name="user_email" value="{$user_email}"></td>
  </tr>
  <tr> 
    <td class="ftitle">Home Phone</td>
    <td><input type="text" name="phone_home" value="{$phone_home}"></td>
  </tr>
  <tr> 
    <td class="ftitle">Other Number</td>
    <td><input type="text" name="phone_other" value="{$phone_other}"></td>
  </tr>
  <tr>
    <td colspan="4" height="10"></td>
  </tr> 
  <tr> 
    <td class="ftitle">Contract Flag Id</td>
    <td><input type="text" name="contract_flag" value="{$contract_flag}"></td>
  </tr>
  <tr> 
    <td class="ftitle">Electronic Signature</td>
    <td><input type="text" name="electronic_signature" value="{$electronic_signature}"></td>
    <td class="ftitle">Electronic Date</td>
    <td>
      <input type="text" id="electronic_date" name="electronic_date" value="{$electronic_date|date_format}" 
        onKeyUp="DateFormat(this,this.value,event,false)" onBlur="DateFormat(this,this.value,event,true)"> 
    <img src="{$image_url}icon_calendar.png" onclick="if(self.gfPop)gfPop.fStartPop(document.getElementById('electronic_date'), document.getElementById('fecha_help'));return false;" >
    </td>    
  </tr>
  <tr>
    <td colspan="4" class="subtitle"></td>
  </tr> 
  <tr> 
    <td class="ftitle">User Nick</td>
    <td><input type="text" name="user_nick" value="{$user_nick}"></td>
    <td class="ftitle">Agreement ID</td>
    <td><input type="text" name="agreement_id" value="{$agreement_id}" readonly></td>  
  </tr>
  <tr> 
    <td class="ftitle">Home App ID</td>
    <td><input type="text" name="home_app_id" value="{$home_app_id}"></td>
    <td class="ftitle">Agreement Date</td>
    <td><input type="text" name="agreement_date" value="{$agreement_date}" readonly></td> 
  </tr>
  <tr> 
    <td class="ftitle">Open New Window</td>
    <td><input type="text" name="open_new_window" value="{$open_new_window}"></td>
    <td class="ftitle">Creation User ID</td>
    <td><input type="text" name="creation_user_id" value="{$creation_user_id}" readonly></td>
  </tr>
  <tr> 
    <td class="ftitle">Modify Date</td>
    <td>
      <input type="text" id="modify_date" name="modify_date" value="{$modify_date|date_format}" 
        onKeyUp="DateFormat(this,this.value,event,false)" onBlur="DateFormat(this,this.value,event,true)"> 
    <img src="{$image_url}icon_calendar.png" onclick="if(self.gfPop)gfPop.fStartPop(document.getElementById('modify_date'), document.getElementById('fecha_help'));return false;" >
    </td>
    <td class="ftitle">Creation Date</td>
    <td>
      <input type="text" id="creation_datetime" name="creation_datetime" value="{$creation_datetime|date_format}" 
        onKeyUp="DateFormat(this,this.value,event,false)" onBlur="DateFormat(this,this.value,event,true)"> 
    <img src="{$image_url}icon_calendar.png" onclick="if(self.gfPop)gfPop.fStartPop(document.getElementById('creation_datetime'), document.getElementById('fecha_help'));return false;" >
    </td>    
  </tr>
  <tr> 
    <td class="ftitle">Internal Email to Mailbox</td>
    <td><input type="text" name="internal_email_to_mailbox" value="{$internal_email_to_mailbox}"></td>
    <td class="ftitle">Delivery Preference</td>
    <td><input type="text" name="tab105_delivery_pref" value="{$tab105_delivery_pref}"></td>    
  </tr>
    
</table> 
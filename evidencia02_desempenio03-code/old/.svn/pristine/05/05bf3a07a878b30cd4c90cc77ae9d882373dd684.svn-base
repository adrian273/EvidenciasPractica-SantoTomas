 /* global */
 .fl{float: left}
 .fr{float: right}
 body { color:#000; font-size:9pt; background: red; overflow: visible!important}

 /* layout */
 #wrap { width:600px;}
 #print_container { width:900px;position: relative }

 /* custom */
 .print-only { display:block !important; }
 .page-break { page-break-before:always; }

 /* links */
 a { text-decoration:underline; color:#999; }

.print_dn{display: none!important}
input,textarea,select{border: none !important;}
input[type=button],input[type=submit]{display: none}
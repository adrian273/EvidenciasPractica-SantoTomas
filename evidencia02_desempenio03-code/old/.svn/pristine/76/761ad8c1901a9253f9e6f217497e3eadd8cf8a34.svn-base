	function get($cms485_id){

		$this->db->select("vt_hha_pn.*, CONCAT(us_agy.first_name,' ', us_agy.last_name) as cm_nurse_name");
		$this->db->join('us_agy','vt_hha_pn.cm_nurse_user_id = us_agy.user_id');
		$this->db->where('cms485_id',$cms485_id);
		$query=$this->db->get('vt_hha_pn');

		return $query->result();

	}

	function get_by_visit_log_id($visit_log_id){

/*
		$this->db->select("vt_hha_pn.*, CONCAT(us_agy.first_name,' ', us_agy.last_name) as cm_nurse_name");
		$this->db->join('us_agy','vt_hha_pn.cm_nurse_user_id = us_agy.user_id');
*/
		$this->db->where('visit_log_id',$visit_log_id);
		$query=$this->db->get('vt_hha_pn');

		return $query->result();

	}

	function get_aidepoc_for_aidepn($cms485_id,$visit_date_time){
/*
		$this->db->select("vt_hha_pn.*, CONCAT(us_agy.first_name,' ', us_agy.last_name) as cm_nurse_name");
		$this->db->join('us_agy','vt_hha_pn.cm_nurse_user_id = us_agy.user_id');
*/
		$this->db->where('cms485_id',$cms485_id);
		$this->db->order_by('hha_poc_effec_date','ASC');
		$rows = $this->db->get('vt_hha_plan_care')->result();
		
		if($rows){
			// tomar el aide poc mas cercano a la fecha de la visita
			foreach($rows as $row){
				if(strtotime($visit_date_time) >= strtotime($row->hha_poc_effec_date)){
					return $row;
				}
			}
		}
		
		return null;
	
	}	

	function insert(){

		$user_id=$this->session->userdata('user_id');

		$this->user_create_id=$user_id;

		$this->date_create_id=date("Y-m-d");

		$this->db->insert('vt_hha_pn', $this);

		$this->hha_progress_note_id = $this->db->insert_id();

		//$this->cms485_id->$cms485_id;

	}

	

	function update($hha_progress_note_id){

		$user_id=$this->session->userdata('user_id');

		$this->user_modify_id=$user_id;

		$this->date_modify_id=date("Y-m-d");

		$this->db->where('hha_progress_note_id',$hha_progress_note_id);

		$this->db->update('vt_hha_pn',$this);

	}

	

	function getById($hha_progress_note_id){

		$this->db->where('hha_progress_note_id',$hha_progress_note_id);

		return $this->db->get('vt_hha_pn')->row();

	}

	

	function delete($hha_progress_note_id){

		$this->db->where('hha_progress_note_id',$hha_progress_note_id);

		$this->db->delete('vt_hha_pn');

	}


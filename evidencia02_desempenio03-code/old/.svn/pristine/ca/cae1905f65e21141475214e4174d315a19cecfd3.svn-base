<table border="0" width="100%" cellpadding="0" cellspacing="4"> 
  <tr>
    <td colspan="2" class="subtitle"><img src="{$image_url}button_small_side.png" style="vertical-align: middle"> Doctor Demographics</td>
  </tr> 
  <tr>
    <td colspan="2" height="10"></td>
  </tr> 
  <tr>
    <td class="ftitle">Doc. First Name</td>
    <td>
      <input type="text" name="first_name" id="first_name" value="{$first_name}"> *
    </td>
  </tr>
  <tr>
    <td class="ftitle">Doc. Last Name</td>
    <td>
      <input type="text" name="last_name" id="last_name" value="{$last_name}"> *
    </td>
  </tr>
  <tr>
    <td class="ftitle">Suffix</td>
    <td>
      <input type="text" name="suffix" id="suffix" value="{$suffix}"> <span style="color:#CCCCCC;">Example: MD, etc</span>
    </td>
  </tr> 
  <tr>
    <td colspan="2" height="10"></td>
  </tr>   
  <tr>
    <td class="ftitle">Address</td>
    <td>
      <input type="text" name="address" id="address" value="{$address}">
    </td>
  </tr> 
  <tr>
    <td class="ftitle">City</td>
    <td>
      <input type="text" name="city" id="city" value="{$city}">
    </td>
  </tr> 
  <tr>
    <td class="ftitle">Zip code</td>
    <td>
      <input type="text" name="zip" id="zip" value="{$zip}">
    </td>
  </tr>
  <tr>
    <td class="ftitle">State</td>
    <td>
      {html_options options=$state_list selected=$state_id name='state_id'}     
    </td>
  </tr>
  <tr>
    <td colspan="2" height="10"></td>
  </tr>   
  <tr>
    <td class="ftitle">Primary Phone</td>
    <td>
      <input type="text" name="phone_primary" id="phone_primary" value="{$phone_primary}">
    </td>
  </tr>
  <tr>
    <td class="ftitle">Secundary Phone</td>
    <td>
      <input type="text" name="phone_secondary" id="phone_secondary" value="{$phone_secondary}">
    </td>
  </tr> 
  <tr>
    <td class="ftitle">Fax</td>
    <td>
      <input type="text" name="fax" id="fax" value="{$fax}">
    </td>
  </tr>
  <tr>
    <td colspan="2" height="10"></td>
  </tr>   
  <tr>
    <td class="ftitle">Contact Name</td>
    <td>
      <input type="text" name="contact_name" id="contact_name" value="{$contact_name}">
    </td>
  </tr>   
  <tr>
    <td class="ftitle">Email</td>
    <td>
      <input type="text" name="user_email" id="user_email" value="{$user_email}"> *
    </td>
  </tr> 
  <tr>
    <td colspan="2" height="10"></td>
  </tr>               
  <tr>
    <td class="ftitle">NPI</td>
    <td>
      <input type="text" name="doctor_office_npi" id="doctor_office_npi" value="{$doctor_office_npi}"> * 
    </td>
  </tr>
  
</table> 
